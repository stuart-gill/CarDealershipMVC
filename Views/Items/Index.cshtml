@using CarDealership.Models;

<h1>Cars:</h1>
<ul>
@if (Model.Count == 0)
{
  <p>You have no cars in your list!</p>
}
@if(Model.Count != 0)
{
  @foreach (Item item in Model)
  {
    <li>Make: @item.GetMakeModel()</li>
    <li>Price: $@item.GetPrice()</li>
    <li>Miles: @item.GetMiles()</li>
    <li>Description: @item.GetDescription()</li>
  }
}
</ul>
<a href="/items/new">Add a new car.</a>